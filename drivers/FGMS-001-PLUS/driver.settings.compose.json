        "zwave": {
          "index": 8,
          "size": 1
        },
        "values": [
          {
            "id": "0",
            "label": {
              "en": "Always active",
              "nl": "Altijd actief"
            }
          },
          {
            "id": "1",
            "label": {
              "en": "Only during day",
              "nl": "Alleen overdag"
            }
          },
          {
            "id": "2",
            "label": {
              "en": "Only during night",
              "nl": "Alleen 's nachts"
            }
          }
        ],
        "type": "dropdown"
      }
