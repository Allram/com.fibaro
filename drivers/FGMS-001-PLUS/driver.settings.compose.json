[
  {
    "type": "group",
    "label": {
      "en": "Motion sensor",
      "nl": "Bewegingssensor"
    },
    "collapsed": false,
    "children": [
      {
        "$extends": "sensitivity",
        "$id": "motion_sensor_sensitivity"
      },
      {
        "id": "motion_sensor_blindtime",
        "type": "number",
        "label": {
          "en": "Motion sensor blindtime",
          "nl": "Bewegingssensor blindtime"
        },
        "zwave": {
          "index": 2,
          "size": 1
        },
        "value": 15,
        "attr": {
          "min": 0,
          "max": 15
        },
        "hint": {
          "en": "Period of time through which the PIR sensor is “blind” (insensitive) to motion. Range: 0 - 15 (0.5s - 8s).",
          "nl": "Periode na detecteren van beweging gedurende de sensor ongevoelig is voor nieuwe bewegingsdetectie. Bereik: 0 - 15 (0,5s - 8s)."
        }
      },
      {
        "id": "motion_pulse_counter",
        "value": "1",
        "label": {
          "en": "Motion detection pulse counter",
          "nl": "Bewegingsdetectie pulse teller"
        },
        "hint": {
          "en": "Amount of moves before motion is reported. A higher value results in a less sensitive motion sensor",
          "nl": "Hoeveelheid bewegingen voordat dit wordt gerapporteerd, een hogere waarde resulteert in een minder gevoelige sensor"
        },
        "zwave": {
          "index": 3,
          "size": 1
        },
        "values": [
          {
            "id": "0",
            "label": {
              "en": "1 pulse",
              "nl": "1 beweging"
            }
          },
          {
            "id": "1",
            "label": {
              "en": "2 pulse",
              "nl": "2 bewegingen"
            }
          },
          {
            "id": "2",
            "label": {
              "en": "3 pulse",
              "nl": "3 bewegingen"
            }
          },
          {
            "id": "3",
            "label": {
              "en": "4 pulse",
              "nl": "4 bewegingen"
            }
          }
        ],
        "type": "dropdown"
      },
      {
        "id": "motion_detection_time_window",
        "value": "2",
        "label": {
          "en": "Motion detection time window",
          "nl": "Tijdvak voor bewegingsdetectie"
        },
        "hint": {
          "en": "Period of time in which the number of moves set in the \"Motion pulse counter\" must be reported. A higher value results in a more sensitive motion sensor. It is not recommended to modify this setting",
          "nl": "Tijdvak waarin de bewegingen aangegeven in de \"Bewegingsdetectie pulse teller\" instelling moeten worden gerapporteerd. Een hogere waarde resulteert in een gevoeligere sensor. Het is afgeraden om deze instelling aan te passen"
        },
        "zwave": {
          "index": 4,
          "size": 1
        },
        "values": [
          {
            "id": "0",
            "label": {
              "en": "4 seconds"
            }
          },
          {
            "id": "1",
            "label": {
              "en": "8 seconds"
            }
          },
          {
            "id": "2",
            "label": {
              "en": "12 seconds"
            }
          },
          {
            "id": "3",
            "label": {
              "en": "16 seconds"
            }
          }
        ],
        "type": "dropdown"
      },
      {
        "$extends": "cancellationDelay",
        "$id": "motion_cancellation_delay"
      },
